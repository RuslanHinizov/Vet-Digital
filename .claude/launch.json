{
  "version": "0.0.1",
  "configurations": [
    {
      "name": "backend",
      "runtimeExecutable": "vetdigital-backend/venv/Scripts/uvicorn",
      "runtimeArgs": ["app.main:app", "--reload", "--port", "8000", "--app-dir", "vetdigital-backend"],
      "port": 8000
