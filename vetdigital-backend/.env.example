# Application
APP_NAME=VetDigital
APP_VERSION=1.0.0
DEBUG=false
SECRET_KEY=your-super-secret-key-change-in-production-min-32-chars
ENVIRONMENT=development

# Database
DATABASE_URL=postgresql+asyncpg://vetdigital:vetdigital_pass@localhost:5432/vetdigital
DATABASE_URL_SYNC=postgresql+psycopg2://vetdigital:vetdigital_pass@localhost:5432/vetdigital

# Redis
REDIS_URL=redis://localhost:6379/0
REDIS_CACHE_URL=redis://localhost:6379/1

# MQTT (EMQX)
MQTT_BROKER_HOST=localhost
MQTT_BROKER_PORT=1883
MQTT_USERNAME=vetdigital
MQTT_PASSWORD=mqtt_pass

# MinIO (File Storage)
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_BUCKET_DOCUMENTS=vetdigital-documents
MINIO_BUCKET_PHOTOS=vetdigital-photos
MINIO_SECURE=false

# JWT
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30
JWT_REFRESH_TOKEN_EXPIRE_DAYS=30
JWT_ALGORITHM=HS256

# Smart Bridge (sb.egov.kz)
SMART_BRIDGE_BASE_URL=https://sb.egov.kz/api/v1
SMART_BRIDGE_API_KEY=your-smart-bridge-api-key
SMART_BRIDGE_SYSTEM_ID=vetdigital

# AnimalID.kz
ANIMALID_BASE_URL=https://www.animalid.kz/api
ANIMALID_API_KEY=your-animalid-api-key

# Firebase (Push Notifications)
FIREBASE_CREDENTIALS_PATH=./firebase-credentials.json

# CORS
CORS_ORIGINS=["http://localhost:3000","http://localhost:8080"]

# Celery
CELERY_BROKER_URL=redis://localhost:6379/2
CELERY_RESULT_BACKEND=redis://localhost:6379/3

# Sync
ISZH_SYNC_INTERVAL_HOURS=6
